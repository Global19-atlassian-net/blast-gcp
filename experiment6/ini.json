{
    "source" :
    {
        "pubsub" :
        {
            "project_id" : "ncbi-sandbox-blast",
            "subscript_id" : "integration-queue-dataproc-subscription-sprint6",
            "use" : "true"
        }
    },

    "blastjni" :
    {
        "num_db_limit" : 16,
        "manifest_root" : "dbs_50M.json"
    },

    "result" :
    {
        "asn1" :
        {
            "bucket" : "blast-results",
            "file"   : "%s/seq-annot.asn",
            "gs_or_hdfs" : "gs"
        },
        "status" :
        {
            "bucket" : "blast-results",
            "file"   : "%s/status.txt"
        }
    },

    "spark" :
    {
        "transfer_files" : [ "libblastjni.so" ],
        "num_executors" : 16,
        "num_executor_cores" : 2,
		"with_locality" : false
    },

    "log" :
    {
        "port" : 10011,
        "request" : true,
        "start" : true,
        "done" : true,
        "final" : true
    }

}
