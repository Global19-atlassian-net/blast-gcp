{
    "appName" : "experiment_mike",
    "source" :
    {
        "pubsub" :
        {
            "project_id" : "ncbi-sandbox-blast",
            "subscript_id" : "blast-test-vartanianmh",
            "use" : "false"
        },
        "socket" :
        {
            "trigger_port" : 10012,
            "use" : "false"
        },
        "hdfs" :
        {
            "dir" : "hdfs:///user/%s/requests",
            "use" : "true"
        }
    },

    "blastjni" :
    {
        "db" :
        [
            {
                "selector" : "nt",
                "bucket"   : "nt_50mb_chunks",
                "pattern"  : "nt_50M",
                "extensions" : [ "nhr", "nin", "nsq" ],
                "num_partitions" : 887,
                "num_locations" : 2
            },
            {
                "selector" : "nr",
                "bucket"   : "nr_50mb_chunks",
                "pattern"  : "nr_50M",
                "extensions" : [ "phr", "pin", "psq" ],
                "num_partitions" : 1086,
                "num_locations" : 1
            }
        ],
        "jni_log_level": "DEBUG",
        "top_n" : 100
    },

    "result" :
    {
        "asn1" :
        {
            "bucket" : "blast-test-vartanianmh"
        },
        "status" :
        {
            "bucket"  : "blast-test-vartanianmh"
        }
    },

    "spark" :
    {
        "transfer_files" : [ "libblastjni.so" ],
        "shuffle_reduceLocality_enabled" : "false",
        "scheduler_fair" : "false",
        "with_locality" : "true",
        "num_executors" : 32,
        "num_executor_cores" : 1,
        "executor_memory" : "1G",
        "locality_wait" : "30s"
    },

    "log" :
    {
        "log_port" : 10011,
        "pref_loc" : "false",
        "part_prep" : "true"
    }
}

