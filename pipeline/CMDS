# build the container with tag dev-102
gcloud container builds submit --config cloudbuild.yaml --substitutions=_PATH=.,_TAG=dev-102 .



# pull image from CR to local cloud shell
gcloud docker -- pull us.gcr.io/ncbi-sandbox-blast/blast-pipeline:dev-102

# ls -lr /app inside the container
docker run us.gcr.io/ncbi-sandbox-blast/blast-pipeline:dev-102 /bin/bash -c "ls -lr /app"

