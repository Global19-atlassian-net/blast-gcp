{
    "source" :
    {
        "pubsub" :
        {
            "project_id" : "ncbi-sandbox-blast",
            "subscript_id" : "spark-test-subscript",
            "use" : "true"
        }
    },

    "blastjni" :
    {
        "xnum_db_limit" : 15,
        "jni_log_level" : "ERROR",
        "lib_name" : "blast_json",
        "manifest_root" : "dbs_50M.json"
    },

    "result" :
    {
        "asn1" :
        {
            "file" : "output2/%s/seq-annot.asn",
            "bucket" : "blastgcp-pipeline-test"
        },
        "status" :
        {
            "file" : "status2/%s/status.txt",
            "bucket"  : "blastgcp-pipeline-test"
        }
    },

    "spark" :
    {
        "log_level" : "ERROR",
        "locailty_wait" : "3s",
        "parallel_jobs": 5,
        "transfer_files" : [ "blast_json" ],
        "num_executors" : 2500,
        "executor_memory" : "1G",
        "num_executor_cores" : 1
    }
}
